{
	"name": "Movie_data_sink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "netflix_bronze_storage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folder_name": {
				"type": "string"
			},
			"file_name": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "netflix/@{dataset().folder_name}",
					"type": "Expression"
				},
				"fileSystem": "bronze"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "cast",
				"type": "String"
			},
			{
				"name": "show_id",
				"type": "String"
			}
		]
	}
}